		photographer : @isOneOf(craft, photographer, photographic_laboratory)
		outdoor shop : @isOneOf(shop, fishing, dive, hunting, outdoor, sports)

//////  	OTHER SHOPS & SERVICES
//		ticket shop : shop=ticket
//		print shop : shop=copyshop
		laundry : shop=laundry OR shop=dry_cleaning
//		vaciador : shop=chemist
		pet shop : shop=pet

//////  	GADGETS, HARDWARES, BUILDERS, FINE CRAFTSMAN

		multimedia : shop=video OR shop=music OR shop=video_games
		computer : shop=computer
		electronics : @isOneOf(shop, electronics, radiotechics, hifi)
		mobile phone : shop=mobile_phone 
		telecom office: office=telecommunication

		hardware : @isOneOf(shop, hardware, doityourself)
		paint shop : shop=paint
		construction supply : shop=trade
		garden center : shop=garden_centre OR craft=gardener
		junkshop : amenity=recycling
//  	sun protection : shop=glass_and_aluminum OR @isOneOf(craft, roofer, sun_protection, window_construction, insulation, hvac)		
//		metal works : @isOneOf(craft, blacksmith, tinsmith, metal_construction, scaffolder)
//		wood works : @isOneOf(craft, carpenter, sawmill)
//		stone works : @isOneOf(craft, stonemason, tiler)
//		electrical works : craft=electrician
//		industrial gas : shop=gas 

//////  	MOBILITY & TRANSPORTATION		
		fuel : amenity=fuel
		parking : amenity=parking
		car dealer : shop=car
		car repair : shop=car_repair
		car parts : shop=car_parts
		car wash : amenity=car_wash
		tire shop : shop=tyres
		motorcycle : shop=motorcycle
		bicycle : shop=bicycle
		bus stop : highway=bus_stop
		bus station : amenity=bus_station
		taxi : amenity=taxi
		tricycle : amenity=tricycle OR amenity=tricycle_station OR amenity=tricycle_terminal
		railway station : railway=station
		mini roundabout : highway=mini_roundabout

//////  	TOURISM		
		tourism accomodation : @isOneOf(tourism, hotel, motel, guest_house, hostel)
		tourism viewpoint : tourism=viewpoint
		tourism museum : tourism=museum
//		leisure resort : leisure=resort
		memorial : historic=memorial

//////  	MISC
		aerodrome : aeroway=aerodrome
		housenumber : [addr:housenumber]

////////////////////////////////////////////////
	areas
		swimming pool : leisure=swimming_pool
		water : natural=water OR waterway=riverbank OR landuse=reservoir OR landuse=basin OR waterway=dock
		beach : natural=beach
		leisure stadium : leisure=stadium
		track : leisure=track
		leisure pitch : leisure=pitch
		playground : leisure=playground
		leisure garden : leisure=garden
		village green : landuse=village_green
		recreation ground : landuse=recreation_ground OR leisure=common
		park : leisure=park
		highway pedestrian : highway=pedestrian
		car park: amenity=parking
		landuse garages : landuse=garages

		residential area : landuse=residential
		landuse retail : landuse=retail
		commercial area : landuse=commercial
		landuse religious : landuse=religious OR landuse=religion
		graveyard : amenity=grave_yard OR landuse=cemetery

		landuse industrial : landuse=industrial 
		water works : man_made=water_works
		quarry : landuse=quarry
		landfill : landuse=landfill
		construction : landuse=construction
		
		prison : amenity=prison
		military : landuse=military OR military=barracks
		
		farm : landuse=farm
		farmyard:landuse=farmyard
		orchard : landuse=orchard
		farmland : landuse=farmland

		forest : landuse=forest OR natural=wood
		national park : boundary=national_park
		meadow : natural=meadow OR landuse=meadow
		wetland : natural=wetland
		nature reserve : leisure=nature_reserve
		grass : landuse=grass
		fell : natural=fell
		allotments : landuse=allotments

		aeroway apron : aeroway=apron
		aeroway terminal : aeroway=terminal


////////////////////////////////////////////////
	lines
		
		highway motorway : highway=motorway OR (highway=construction AND construction=motorway)
		highway motorway link : highway=motorway_link OR (highway=construction AND construction=motorway_link)
		highway trunk : highway=trunk OR (highway=construction AND construction=trunk)
		highway trunk link: highway=trunk_link OR (highway=construction AND construction=trunk_link)
		highway primary link : highway=primary_link OR (highway=construction AND construction=primary_link)
		highway primary : highway=primary OR (highway=construction AND construction=primary)
		highway secondary : highway=secondary OR highway=secondary_link OR (highway=construction AND construction=secondary)
		highway tertiary : highway=tertiary OR highway=tertiary_link OR (highway=construction AND construction=tertiary)
		highway unclassified : highway=unclassified OR (highway=construction AND construction=unclassified)
		highway residential : highway=residential OR (highway=construction AND construction=residential)
		highway living_street : highway=living_street OR (highway=construction AND construction=living_street)
		highway construction : highway=construction
		highway service : highway=service
		highway pedestrian : highway=pedestrian
		highway track : highway=track
		highway footway : highway=footway
		highway cycleway : highway=cycleway
		highway path : highway=path
		highway steps : highway=steps
//		highway access private : access=private AND highway
//		highway access destination : access=destination AND highway
		oneway : oneway=yes OR oneway=true OR oneway=-1
		bridge foot : bridge=yes AND (foot=yes OR highway=footway)
		historic citywalls : historic=citywalls OR historic=city_wall
		line barrier : barrier=wall OR barrier=fence
		coastline : natural=coastline
		river : waterway=river
		stream : waterway=stream
		canal : waterway=canal
		drain : waterway=drain
		dock : waterway=dock
		pier : man_made=pier
		railway rail : railway=rail
		railway lightrail : railway=light_rail
		platform : railway=platform OR public_transport=platform

		power_line : power=line OR power=minor_line
		aeroway runway : aeroway=runway
		aeroway taxiway : aeroway=taxiway

		boundary country : boundary=administrative AND (admin_level=2 OR admin_level=4) AND NOT natural=coastline
		boundary province : boundary=administrative AND admin_level=6
		boundary municipal : boundary=administrative AND admin_level=8
		boundary barangay : boundary=administrative AND admin_level=10
		
		contour major : contour[@isMulti(elevation, 100)]
		contour minor : contour[@isMulti(elevation, 20) and not @isMulti(elevation, 100)]

		gps tracks : gpstrack[]
		gps routes : gpsroute[]


		
// Routes
//		route ncn : relation[type=route AND route=bicycle AND network=ncn]
		route byway : relation[type=route AND route=bicycle AND network="National Byway"]
		route foot : relation[type=route AND route=foot]

		
// ==========================================================================
properties

	map-background-color	: #F7F4ED
	map-background-opacity	: 1
	map-sea-color : #B5D0D0
	font-weight : bold
	font-family : Verdana
	text-max-width : 7
	text-halo-width : 25%
	text-halo-opacity : 0.75
	text-align-horizontal : center
	text-align-vertical : center
	font-stretch : 0.9
	map.rendering.lflp.min-buffer-space : 5
	map.rendering.lflp.max-allowed-corner-angle : 40




	
// ==========================================================================
rules

////// PLACES
	target : place*
		define	
			font-weight : bold	
			text-max-width : 9	
			text-align-horizontal : center	
		if : *city	
			define	
				font-size : 6:8;14:20	
				min-zoom : 6	
				max-zoom : 14	
				placement-value : 1000	
		elseif : *town	
			define	
				font-size : 9:10;12:15;20:25	
				min-zoom : 9	
				max-zoom : 15		
				placement-value : 900	
		elseif : *barangay	
			define
				text : @up(name)
				text-color : white black 60%				
				font-weight : bold
				font-size : 12:9;14:12;18:18
				min-zoom : 12
				max-zoom : 18	
				placement-value : 700	
		elseif : *suburb	
			define	
				font-size : 13:10;20:20
				min-zoom : 13	
				text-color : white black 40%	
				placement-value : 10	
		elseif : *neighbourhood	
			define	
				font-size : 14:8;16:10;18:15;20:20
				font-style : italic
				min-zoom : 14
				text-color : #8F5E5E
//				text-color : green blue 50%	
				placement-value : 1	
		elseif : *locality	
			define	
				font-size : 14:8;20:16
				text-color : red	
				min-zoom : 14	
				placement-value : 0.1
		elseif : *hamlet	
			define	
				font-size : 14:8;20:16
				text-color : red		
				min-zoom : 14	
				placement-value : 0.1			
		elseif : *subdivision	
			define	
				font-size : 14:8;20:16
				text-color : red	
				min-zoom : 14	
				placement-value : 0.1			
		draw : text	

		
	
		
		
////// HOUSENUMBER		
	target : housenumber
		define
			min-zoom : 18
			text : [[addr:housenumber]]
			text-halo-width : 0
			font-size : 8
			placement-value : 0.01
		draw : text

///////	AREAS		
	target : $featuretype(area)
		define
			line-style : none
			line-width : 1
		if : forest
			define
				min-zoom : 9
				fill-color : #8DC56C
				fill-opacity : 0.3
		elseif : *pedestrian
			for : area
				define
					min-zoom : 14
					fill-color : #EDEDED
					line-color : #C4C4C4
					line-style : solid		
		elseif : swimming pool
			define
				fill-color : #B5D0D0
		elseif : beach
			define
				min-zoom : 10
				fill-color : #FEFEC0				
		elseif : playground
			define
				fill-color : #CCFEF0
			define
				icon-image : icons/SJJB/png/sport_playground.n.32.png
				icon-width : 16:2;19:16
				min-zoom : 16
			draw : icon	
		elseif : recreation ground
			define
				fill-color : #CCEEBB				
		elseif : farmland
			define
				fill-color : #D6D6A6
				fill-opacity : 0.4
		elseif : farm
			define
				min-zoom : 9
				fill-color : #DBDFB6
				fill-opacity : 0.4
		elseif : farmyard
			define
				min-zoom : 9
				fill-color : #DBDFB6
		elseif : orchard
			define
				fill-color : #9fd790
				fill-opacity : 0.4
		elseif : fell
			define
				fill-color : #C5FF5B black 10%
		elseif : water
			define
				fill-color : #B5D0D0
		elseif : grass
			define
				fill-color : #CFECA8
		elseif : allotments
			define
				fill-color : #C8B084
		elseif : meadow
			define
				fill-color : #CFECA8
		elseif : wetland
			define
				fill-color : #C5E9E9
				fill-opacity : 0
				fill-hatch : diagonalcross
				fill-hatch-color : #B5D0D0
				fill-hatch-opacity : 0.5
		elseif : nature reserve
			define
				fill-color : #ABDE96
		elseif : car park
			define
				fill-color : #F2E9C2	
//				fill-color : #FFFEFA  (lighter gray)
//				fill-color : #F9F7F2 (light gray) 
		elseif : park
			define
				fill-color : #CCEEBB
		elseif : leisure pitch
			define
				fill-color : #89D2AE
		elseif : leisure garden
			define
				fill-color : #CFECA8
		elseif : graveyard
			define
				fill-color : #DDEECC
		elseif : leisure stadium
			define
				fill-color : #33CC99
		elseif : track
			define
				fill-color : #74DCBA
		elseif : village green
			define
				fill-color : #CFECA8
		elseif : landuse retail
			define
				fill-color : #F2E3D3
			draw : line
		elseif : commercial area
			define
				fill-color : #F2E3D3
		elseif : landuse religious
			define
				fill-color : #EEFBE2
		elseif : industrial plant 
			define
				fill-color : #D6D6D6				
		elseif : landuse industrial 
			define
				fill-color : #D6D6D6
		elseif : residential area
			define
				fill-color : #ECECE7
		elseif : water works
			define
				fill-color : #CECECE
		elseif : landuse garages
			define
				fill-color : #F9F7F2
		elseif : prison
			define
				fill-color : #CECECE
		elseif : military
			define
				fill-color : #D7DBBA
		elseif : quarry
			define
				fill-color : white
				fill-hatch : diagonalcross
				fill-hatch-color : #cccccc
		elseif : landfill
			define
				fill-color : yellow
				fill-opacity : 0.2
				fill-hatch : diagonalcross
				fill-hatch-color : #DFD3C6			
		elseif : construction			
			define
				fill-color : #e9e8e3
				fill-hatch : swne
				fill-hatch-color : #cccccc						
		elseif : aeroway*
			define
				fill-color : #E9D1FE
		elseif : aerodrome
			define
				fill-color : #e9e8e3
				line-style : solid
				line-color : #b3b3b0 black 10%
		elseif: academic
			define
				fill-color : #EEEEC3
				min-zoom : 16
		elseif : community center
			define
				fill-color : #99B990
		elseif : public building
			define
				fill-color : #CFAE37
		elseif : building
			define
				fill-color : #D8CBBC
		else
			stop
		draw : fill

//////  ADD DEMARCATION LINES to polygons	
		if : building
			define
				min-zoom : 18
				line-style : solid
				line-width : 1
				line-color : #666
		elseif : academic
			define
				min-zoom : 17
				line-color : #E5D2CC
				line-style : solid
		elseif : industrial plant
			define	
				min-zoom : 16	
				line-style : dot
				line-width : 1.5
				line-color : #F1EEE8				
		elseif : landuse industrial
			define	
				min-zoom : 16	
				line-style : dot
				line-width : 1.5
				line-color : #F1EEE8	
		elseif : commercial area
			define	
				min-zoom : 16	
				line-style : dot
				line-width : 1.5
				line-color : #F1EEE8					
		elseif : residential area
			define		
				line-style : dot
				line-width : 2
				line-color : gray white 75%
		elseif : graveyard
			define
				min-zoom : 16
				line-style : solid
				line-width : 1
				line-color : #D6D6AB
		elseif : quarry
			define				
				line-style : solid
				line-color : #b3b3b0 black 10%
		draw : line
			
//////	TEXT DECLUTTER: suppress name of buildings & landuse polygons at scale 1:250m
		define
			min-zoom : 17		
//////  TEXT REMOVE / SUPPRESS: redundant-polygon-name or building tag, if a named-icon exist		
		for : @isOneOf(amenity, school, kindergarten, university, college, library)
			stop
		for : @isOneOf(amenity, hospital, clinic, doctors, dentist, health_center, health_centre) 
			stop
		for : @isOneOf(amenity, pharmacy, nursing_home, veterinary, fitness_center)	
			stop
		for : @isOneOf(amenity, bank, place_of_worship, fuel, car_wash, recycling)
			stop	
		for : @isOneOf(shop, supermarket, convenience, bakery, butcher, seafood)
			stop
		for : @isOneOf(tourism, hotel, motel, guest_house, hostel, museum)
			stop
		for : @isOneOf(amenity, restaurant, fast_food, bar, pub) 
			stop
		for : @isOneOf(shop, car, car_repair, motorcycle, bicycle)
			stop
		for : @isOneOf(shop, hardware, doityourself, paint, trade, glass_and_aluminum, garden_centre)
			stop
		for : shop=mobile OR office=telecommunication
			stop	
		for : @isOneOf(office, government, ngo, telecommunication)
			stop			
		for : landuse=residential AND place=neighbourhood
			stop
		for : landuse=garages
			stop
		for : emergency=ses_station OR amenity=emergency_services
			stop	
//////  TEXT: CUSTOMIZE FONT of polygons
		for : amenity=townhall
			define
//				min-zoom : 15
				min-zoom : 12
				font-weight : bold
				font-size : 15:10;16:10;17:11;18:13
				text-color : green blue 40%
				placement-value : 100	
		for : amenity=fuel
			define
				font-size : 10
				text-color : #8888FF
		for : shop=mall OR amenity=marketplace
			define
				min-zoom : 16
				font-weight : bold
				font-size : 16:8;17:9;18:10
				text-color : brown
		for : landuse=recreation_ground
			define
				min-zoom : 15
